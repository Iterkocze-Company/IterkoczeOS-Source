NAME="libmagic"
DESC="Debian Libmagic (Obsolete)"
SRC_URL="http://ftp.de.debian.org/debian/pool/main/f/file/libmagic1_5.39-3_amd64.deb"
DEPENDSON="libmagic-mgc"
OBSOLETE

BEGIN INSTALL
	cd etc
	mv -v magic /etc/
	mv -v magic.mime /etc/

	cd ../usr/lib/x86_64-linux-gnu
	mv -v libmagic.so.1 /usr/local/lib/
	mv -v libmagic.so.1.0.0 /usr/local/lib/
END INSTALL

BEGIN REMOVE
	rm -v /usr/local/lib/libmagic.so.1
	rm -v  /usr/local/lib/libmagic.so.1.0.0
END REMOVE
