<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="toplevel1">
    <property name="height">200</property>
    <property name="width">400</property>
    <child>
      <object class="ttk.Frame" id="frame1">
        <property name="height">200</property>
        <property name="width">400</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.0</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label1">
            <property name="font">TkTextFont</property>
            <property name="justify">center</property>
            <property name="text" translatable="yes">Welcome to IterkoczeOS!</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label2">
            <property name="text" translatable="yes">Don't forget to check out the README file in your home directory</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="rely">0.1</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="closeBtn" named="True">
            <property name="cursor">arrow</property>
            <property name="text" translatable="yes">OK</property>
            <bind sequence="&lt;ButtonPress&gt;" handler="ok_clicked" add="" />
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.0</property>
              <property name="rely">0.85</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
